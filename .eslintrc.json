{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:sonarjs/recommended", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "jest/globals": true,
    "browser": true
  },
  "globals": {
    "process": true,
    "Promise": true
  },
  "plugins": ["prettier", "jest", "sonarjs"],
  "rules": {
    "prettier/prettier": ["error"],
    "quotes": [2, "double"],
    "comma-dangle": [2, "never"],
    "react/prop-types": [0]
  }
}
